<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="astina_solvency.provider.class">Astina\Bundle\SolvencyBundle\Provider\Mock\Provider</parameter>
        <parameter key="astina_solvency.provider.status">good</parameter>
    </parameters>

    <services>
        <service id="astina_solvency.provider" class="%astina_solvency.provider.class%">
            <argument>%astina_solvency.provider.status%</argument>
        </service>
    </services>
</container>
